  "scripts": {
    "build": "tsc",
    "start": "bun run src/helpers/reset.ts && bun run db:push && bun src/main.ts",
    "start:http-server": "export RUN_MODE=http-server && bun run start",
    "db:push": "drizzle-kit push --force",
    "format": "prettier --write .",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:clean-unused-vars": "eslint --rule 'no-unused-vars: [\"error\", { \"args\": \"none\" }]' --quiet -f json | remove-unused-vars",
    "lint:fix-all": "bun run lint:clean-unused-vars && bun run lint:fix"
  }
