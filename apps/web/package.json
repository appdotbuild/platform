{
  "name": "@app.build/web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "clean": "rm -rf node_modules dist tmp",
    "dev": "vite",
    "e2e:codegen": "playwright codegen --load-storage=./e2e/sessions/storageState.json",
    "e2e:codegen:logged-out": "playwright codegen",
    "e2e:test": "playwright test",
    "e2e:ui": "playwright test --ui",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write .",
    "otp": "bun --env-file=./apps/web/.env run tools/get-otp.ts",
    "preview": "vite preview",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "@appdotbuild/auth": "workspace:*",
    "@appdotbuild/core": "workspace:*",
    "@segment/analytics-next": "^1.81.1",
    "@stackframe/react": "^2.8.16",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-router": "^1.123.0",
    "lucide-react": "^0.525.0",
    "react": "catalog:",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tanstack/react-query-devtools": "^5.81.5",
    "@tanstack/router-cli": "^1.123.0",
    "@tanstack/router-devtools": "^1.123.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0"
  }
}
